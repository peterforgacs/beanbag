  "bugs": {
    "url": "https://github.com/peterforgacs/beanbag/issues"
  },
  "homepage": "https://github.com/peterforgacs/beanbag#readme",
  "devDependencies": {
    "@types/node": "^14.0.11",
    "cz-conventional-changelog": "^3.2.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "standard-version": "^8.0.1",
    "typescript": "^3.9.5",
    "@types/jest": "^25.2.3"
  },
  "config": {
